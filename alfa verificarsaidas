#include <stdio.h>  //biblioteca padrão da linguaguem C

int main(){
	
	int boleano;
	
	float a = 1.547;
	float b = 3.276;
	float c = 4.823;
	float d;
	
	d = a + b;
	
	if (c == d){
		boleano = 1;
	} else {
		boleano = 0;
	}
	
	if(boleano){
		printf("\n\nIgual\n");
	} else {
		printf("\n\nDiferente\n");
	}
	
 return 0;
}
