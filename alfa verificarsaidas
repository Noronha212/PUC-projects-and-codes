#include <stdio.h>  //biblioteca padrão da linguaguem C

//Declaração do tipo boleano/lógico
#ifndef _Booleano_h
        #define _Booleano_h
        typedef enum{FALSE,TRUE} bool;
#endif

bool vfc; //Declaraçaõ da variável boleana vfc(verificar)

int main(){
	
	float a = 1.547;
	float b = 3.276;
	float c = 2.823;
	float d;
	
	d = a + b;
	
	if (c == d){
		vfc = TRUE;
	} else {
		vfc = FALSE;
	}
	
	if(vfc){
		printf("\n\nIgual\n");
	} else {
		printf("\n\nDiferente\n");
	}
	
 return 0;
}
